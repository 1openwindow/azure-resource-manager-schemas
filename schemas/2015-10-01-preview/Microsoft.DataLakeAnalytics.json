{
	"id": "http://schema.management.azure.com/schemas/2015-10-01-preview/Microsoft.DataLakeAnalytics.json#",
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Microsoft.DataLakeAnalytics",
	"description": "Microsoft Data Lake Analytics Resource Types",
	"resourceDefinitions": {
		"accounts": {
			"type": "object",
			"description": "Microsoft.DataLakeAnalytics/accounts",
			"properties": {
                "apiVersion": {
                    "enum": [
                        "2015-10-01-preview"
                    ],
					"description": "Microsoft.DataLakeAnalytics/accounts - API version"
                },
				"type": {
					"enum": ["Microsoft.DataLakeAnalytics/accounts"],
					"description": "Microsoft.DataLakeAnalytics/accounts - ARM resource type"
				},
				"location": {
					"enum": ["East US 2"],
					"description": "Microsoft.DataLakeAnalytics/accounts - Resource location"
				},
				"properties": {
					"type": "object",
					"description": "Microsoft.DataLakeAnalytics/accounts - Resource properties",
					"properties": {
						"defaultDataLakeStoreAccount": {
							"type": "string",
							"description": "Microsoft.DataLakeAnalytics/accounts - Default Data Lake Store account",
							"minLength": 3,
							"maxLength": 24,
							"pattern": "^[a-z0-9]+$"
						},
						"dataLakeStoreAccounts": {
							"type": "array",
							"description": "Microsoft.DataLakeAnalytics/accounts - List of Data Lake Store accounts",
							"items": {
								"type": "object",
								"description": "Microsoft.DataLakeAnalytics/accounts - Data Lake Store account info",
								"properties": {
									"name": {
										"type": "string",
										"description": "Microsoft.DataLakeAnalytics/accounts - Data Lake Store account name",
										"minLength": 3,
										"maxLength": 24,
										"pattern": "^[a-z0-9]+$"
									}
								},
                                "required": [
                                    "name"
                                ],
                                "additionalProperties": false
							}
						},
						"storageAccounts": {
							"type": "array",
							"description": "Microsoft.DataLakeAnalytics/accounts - List of Azure Storage accounts",
							"items": {
								"type": "object",
								"description": "Microsoft.DataLakeAnalytics/accounts - Azure Storage account info",
								"properties": {
									"name": {
										"type": "string",
										"description": "Microsoft.DataLakeAnalytics/accounts - Azure Storage account name",
										"minLength": 3,
										"maxLength": 50,
										"pattern": "^[a-zA-Z0-9_-]+$"
									},
									"properties": {
										"type": "object",
										"description": "Microsoft.DataLakeAnalytics/accounts - Azure Storage account info properties",
										"properties": {
											"accessKey": {
												"type": "string",
												"description": "Microsoft.DataLakeAnalytics/accounts - Azure Storage account access key"
											}
										},
                                        "required": [
                                            "accessKey"
                                        ],
                                        "additionalProperties": false
									}
								},
                                "required": [
                                    "name",
                                    "properties"
                                ],
                                "additionalProperties": false
							}
						}
					},
                    "required": [
                        "defaultDataLakeStoreAccount",
                        "dataLakeStoreAccounts"
                    ]
				}
			},
			"required": ["apiVersion",
			"type",
			"location",
			"properties"]
		}
	}
}